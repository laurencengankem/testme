pipeline {

  agent any
  
  stages {
  
 
    
    stage("build") {
    
      steps {
        echo "building out to the branch"
      }
    
    }
    
    stage("test") {
    
      steps {
        echo "testing out to the branch"
      }
    
    }
    
    stage("deploy") {
    
      steps {
        echo "deploying out to the branch"
      }
    
    }    
    
  
  }

}
